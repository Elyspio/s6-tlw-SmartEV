{
    "name": "ts-node-template",
    "version": "1.0.0",
    "author": "Jonathan Guichard",
    "license": "MIT",
    "private": false,
    "devDependencies": {
        "@babel/cli": "latest",
        "@babel/core": "latest",
        "@babel/preset-env": "latest",
        "@types/express": "^4.17.2",
        "@types/node": "^13.7.1",
        "@types/node-fetch": "^2.5.4",
        "nodemon": "latest",
        "prettier": "latest",
        "tslint": "latest",
        "typescript": "latest",
        "pkg": "4.4.3"
    },
    "dependencies": {
        "express": "latest",
        "node-fetch": "^2.6.0"
    },
    "scripts": {
        "watch": "nodemon -e ts,json --watch ./src -x \"npm run build && node lib/app.js\"",
        "build": "tsc && babel build --out-dir lib",
        "dist": "npm run build && pkg ./lib/app.js --target node8-win-x64"
    }
}
